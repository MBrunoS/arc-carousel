{
  "name": "arc-carousel",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/arc-carousel.umd.js",
  "module": "./dist/arc-carousel.es.js",
  "exports": {
    ".": {
      "require": "./dist/arc-carousel.umd.js",
      "import": "./dist/arc-carousel.es.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && npm run build:css",
    "build:css": "tailwindcss -m -i ./src/global.css -o ./dist/styles.css",
    "prepublishOnly": "npm run build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "test": "vitest",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "pnpm run lint",
      "pnpm run format"
    ]
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/ui": "^0.34.5",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hooks": "^1.0.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.4.0",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbrunos/arc-carousel"
  },
  "keywords": [],
  "author": "MBrunoS",
  "bugs": {
    "url": "https://github.com/mbrunos/arc-carousel/issues"
  },
  "homepage": "https://github.com/mbrunos/arc-carousel#readme",
  "license": "MIT"
}
